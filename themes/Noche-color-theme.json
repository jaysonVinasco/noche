{
    "name": "Noche",
    "type": "dark",
    "colors": {
        "editor.background": "#0d0913",
        "editor.foreground": "#68a9d7",
        "progressBar.background": "#8c7fda",

        "activityBar.background": "#0d0913",
        "activityBarBadge.background": "#144745",
        "activityBar.activeBackground": "#144745",

        "sideBar.background": "#0d0913",
        "sideBar.border": "#39364e",

        "list.activeSelectionBackground": "#144745",
        "list.focusBackground": "#144745",

        "statusBar.background": "#12101b",

        "titleBar.activeBackground": "#12101b",
        "menu.background": "#12101b",

        "button.background": "#144745",
        "textLink.foreground": "#19b3ad",

        "tab.activeBackground": "#144745",

        "editorLineNumber.activeForeground": "#08f5ed", 
        "editor.selectionBackground": "#0b3b3a",
        "editor.lineHighlightBackground": "#261435",
        "editor.linkedEditingBackground": "#171236",
        "editor.hoverHighlightBackground": "#0a0c0a",
        "editor.wordHighlightBackground": "#000000",

    },
    "tokenColors": [
        {
            "name": "Comment",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
            
                "foreground": "#473266"
            }
        },
        {
            "name": "Variables",
            "scope": ["variable", "string constant.other.placeholder"],
            "settings": {
                "foreground": "#706f6f",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Colors",
            "scope": ["constant.other.color"],
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Invalid",
            "scope": ["invalid", "invalid.illegal"],
            "settings": {
                "foreground": "#FF5370"
            }
        },
        {
            "name": "Keyword, Storage",
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": {
                "foreground": "#8c76e7",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Operator, Misc",
            "scope": [
                "keyword.control",
                "constant.other.color",

                "meta.tag",

                "punctuation.section.embedded",
                "keyword.other.template",
                "keyword.other.substitution"
            ],
            "settings": {
                "foreground": "#99f5fc",
                "fontStyle": "bold underline"
            }
        },
        {
            "name": "Tag",
            "scope": [
                "entity.name.tag",
                "meta.tag.sgml",
                "markup.deleted.git_gutter"
            ],
            "settings": {
                "foreground": "#2da59f",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Function, Special Method",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "variable.function",
                "support.function",
                "keyword.other.special-method"
            ],
            "settings": {
                "foreground": "#13b76a",
             
            }
        },
        {
            "name": "Block Level Variables",
            "scope": ["meta.block variable.other"],
            "settings": {
                "foreground": "#f8caa4"
            }
        },
        {
            "name": "Other Variable, String Link",
            "scope": ["support.other.variable", "string.other.link"],
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.constant",
                "constant.character",
                "constant.escape",
                "variable.parameter",
                "keyword.other.unit",
                "keyword.other"
            ],
            "settings": {
                "foreground": "#f30799"
            }
        },
        {
            "name": "String, Symbols, Inherited Class, Markup Heading",
            "scope": [
                "string",
                "constant.other.symbol",
                "constant.other.key",
                "entity.other.inherited-class",
                "markup.heading",
                "markup.inserted.git_gutter",
                "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
            ],
            "settings": {
                "foreground": "#ee7e9f",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Class, Support",
            "scope": [
                "entity.name",
                "support.type",
                "support.class",
                "support.other.namespace.use.php",
                "meta.use.php",
                "support.other.namespace.php",
                "markup.changed.git_gutter",
                "support.type.sys-types"
            ],
            "settings": {
                // "foreground": "#4cab65",
                "foreground": "#b1b059",
                "fontStyle": "bold"
       
          
            }
        },
        {
            "name": "Entity Types",
            "scope": ["support.type"],
            "settings": {
                "foreground": "#2da59f",
                "fontStyle": "bold"
            }
        },
       
    ]
}
